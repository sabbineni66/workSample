  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.7.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.3"
  }
