{
	// Reference https://code.visualstudio.com/docs/getstarted/theme-color-reference
	"name": "macOS Classic",
	"author": "Jason Lee",
	"maintainers": [
			"Jason Lee <huacnlee@gmail.com>"
	],
	"type": "light",
	"semanticClass": "theme.macos-classic",
	"colors": {
	"scrollbar.shadow": "#ffffff",
	"editor.background": "#ffffff",
	"editor.foreground": "#24292e",
	"editor.lineHighlightBackground": "#fffbdd",
	"editorIndentGuide.background": "#eeeeee",
	"editorLineNumber.foreground": "#929292",
	"editorLineNumber.background": "#e4e4e4",
	"editorBracketMatch.background": "#f1f8ff",
	"editorBracketMatch.border": "#c8e1ff",
	"editorGroupHeader.tabsBackground": "#c4c4c4",
	"editorGroupHeader.tabsBorder": "#a09ea0",
	"editorGroup.border": "#a09ea0",
	"tab.activeBackground": "#d8d6d8",
	"tab.inactiveBackground": "#c4c4c4",
	"tab.border": "#a09ea0",
	"tab.activeBorder": "#d8d6d8",
	"tab.activeForeground": "#262426",
	"tab.inactiveForeground": "#575557",
	"statusBar.foreground": "#575557",
	"statusBar.noFolderForeground": "#575557",
	"statusBar.background": "#ededed",
	"statusBar.noFolderBackground": "#ededed",
	"statusBar.border": "#c4c4c4",
	"activityBar.background": "#d6d4d6",
	"activityBarBadge.background": "#d6d4d6",
	"activityBar.border": "#a09ea0",
	"activityBar.foreground": "#262426",
	"activityBarBadge.foreground": "#262426",
	"sideBar.background": "#ffffff",
	"sideBar.foreground": "#262426",
	"sideBar.border": "#a09ea0",
	"sideBarTitle.foreground": "#24292e",
	"titleBar.activeBackground": "#c4c4c4",
	"titleBar.border": "#a09ea0",
	"notification.background": "#54a3ff",
	"statusBar.debuggingBackground": "#fafbfc",
	"statusBar.debuggingForeground": "#24292e",
	"panel.background": "#ffffff",
	"panel.border": "#a09ea0",
	"panelTitle.activeForeground": "#24292e",
	"input.background": "#ffffff",
	"input.foreground": "#262426",
	"input.border": "#e0e0e0",
	"focusBorder": "#eeeeee",
	"dropdown.background": "#ffffff",
	"dropdown.border": "#c4c4c4",
	"dropdown.foreground": "#262426",
	"scrollbarSlider.background": "#f0f0f0",
	"scrollbarSlider.hoverBackground": "#f0f0f0",
	"scrollbarSlider.activeBackground": "#f0f0f0",
	"list.hoverBackground": "#ffffff",
	"list.hoverForeground": "#262426",
	"list.activeSelectionBackground": "#0069d9",
	"list.activeSelectionForeground": "#ffffff",
	"list.inactiveSelectionBackground": "#dcdcdc",
	"list.inactiveSelectionForeground": "#262426",
	"editorCursor.foreground": "#333333",
	"selection.background": "#e5f1ff",
	"editor.selectionBackground": "#e5f1ff",
	"editor.findMatchHighlightBackground": "#e5f1ff",
	"editor.findMatchBackground": "#e5f1ff",
	"editor.wordHighlightBackground": "#e5f1ff"
},
"tokenColors": [
	{
		"settings": {
			"foreground": "#24292eff",
			"background": "#ffffffff"
		}
	},
	{
		"scope": "emphasis",
		"settings": {
			"fontStyle": "italic"
		}
	},
	{
		"scope": [
			"strong",
			"markup.heading.markdown",
			"markup.bold.markdown"
		],
		"settings": {
			"fontStyle": "bold"
		}
	},
	{
		"scope": [
			"markup.italic.markdown"
		],
		"settings": {
			"fontStyle": "italic"
		}
	},
	{
		"scope": "meta.link.inline.markdown",
		"settings": {
			"fontStyle": "underline",
			"foreground": "#005cc5"
		}
	},
	{
		"scope": [
			"comment",
			"markup.fenced_code",
			"markup.inline"
		],
		"settings": {
			"foreground": "#007fff"
		}
	},
	{
		"scope": "string",
		"settings": {
			"foreground": "#1b8736"
		}
	},
	{
		"scope": [
			"constant.numeric",
			"constant.language",
			"variable.other.class",
			"variable.other.constant",
			"meta.property-name",
			"meta.property-value",
			"support"
		],
		"settings": {
			"foreground": "#8090e5"
		}
	},
	{
		"scope": [
			"keyword",
			"storage.modifier",
			"storage.type",
			"support.function.node",
			"support.function",
			"variable.language.this",
			"punctuation.definition.template-expression"
		],
		"settings": {
			"foreground": "#0433ff"
		}
	},
	{
		"scope": [
			"keyword.operator.accessor",
			"meta.group.braces.round.function.arguments",
			"meta.template.expression"
		],
		"settings": {
			"foreground": "#24292eff"
		}
	},
	{
		"scope": [
			"entity.name.type",
			"entity.other.inherited-class",
			"variable.other.object.property",
			"meta.instance.constructor"
		],
		"settings": {
			"foreground": "#6f42c1"
		}
	},
	{
		"scope": [
			"meta.function-call",
			"entity.name.function",
			"variable.parameter.function",
			"entity.other.attribute-name",
			"support.variable.property.dom",
			"support.type.property-name.json",
			"punctuation.separator.key-value"
		],
		"settings": {
			"foreground": "#333333"
		}
	},
	{
		"scope": [
			"variable.other.constant",
			"variable.language.this"
		],
		"settings": {
			"foreground": "#3b96a6"
		}
	},
	{
		"scope": [
			"constant.other.symbol"
		],
		"settings": {
			"foreground": "#d21f07"
		}
	},
	{
		"scope": [
			"entity.name.tag",
			"string.quoted",
			"string.regexp",
			"string.interpolated",
			"string.template",
			"keyword.other.template"
		],
		"settings": {
			"foreground": "#1b8736"
		}
	},
	{
		"scope": "token.info-token",
		"settings": {
			"foreground": "#316bcd"
		}
	},
	{
		"scope": "token.warn-token",
		"settings": {
			"foreground": "#cd9731"
		}
	},
	{
		"scope": "token.error-token",
		"settings": {
			"foreground": "#cd3131"
		}
	},
	{
		"scope": "token.debug-token",
		"settings": {
			"foreground": "#800080"
		}
	}
]
}
