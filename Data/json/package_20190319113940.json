{
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "name": "react-skeleton",
  "version": "0.1.0",
  "author": "R?gis Gaidot <rgaidot@gmail.com>",
  "license": "MIT",
  "description": "A simple skeleton for create a project with React, Redux and SASS",
  "keywords": [
    "webpack",
    "react",
    "redux",
    "mocha",
    "es6",
    "starter",
    "boilerplate"
  ],
  "scripts": {
