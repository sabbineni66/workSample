    "babel-polyfill": "6.9.0",
    "isomorphic-fetch": "2.2.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
  "babel": {
    "presets": [
      "react",
      "node5",
      "stage-0"
